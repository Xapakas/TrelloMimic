<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.VBox?>

<TitledPane maxHeight="-Infinity" maxWidth="-Infinity" minWidth="150.0" stylesheets="@fun.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.ListController">
   <content>
      <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #65743A;">
         <children>
            <Accordion id="cardAccordion" fx:id="cardAccordion" />
            <Button id="addNewCardButton" fx:id="addNewCardButton" mnemonicParsing="false" onAction="#onClickAddNewCard" text="Add New Card" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </content>
   <graphic>
      <Label id="listNameLabel" fx:id="listNameLabel" text="[list name]" />
   </graphic>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</TitledPane>
